<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="annaikanyouzoo.com.outerspacemanager.outerspacemanager">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/material_dark_orange_light">
        <activity android:name=".activities.SignUpActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.MainActivity"
            android:label="@string/app_name" />
        <activity android:name=".activities.BuildingActivity"
            android:parentActivityName="annaikanyouzoo.com.outerspacemanager.outerspacemanager.activities.MainActivity"
            android:label="@string/app_name" />
        <activity android:name=".activities.LoginActivity" />
        <activity android:name=".activities.UserActivity"
            android:parentActivityName="annaikanyouzoo.com.outerspacemanager.outerspacemanager.activities.MainActivity"
            android:label="@string/app_name"/>
        <activity android:name=".activities.ShipsActivity"
            android:parentActivityName="annaikanyouzoo.com.outerspacemanager.outerspacemanager.activities.MainActivity"
            android:label="@string/app_name"/>
        <activity android:name=".activities.FleetActivity"
            android:parentActivityName="annaikanyouzoo.com.outerspacemanager.outerspacemanager.activities.MainActivity"
            android:label="@string/app_name"/>
        <activity android:name=".activities.AttacksActivity"
            android:parentActivityName="annaikanyouzoo.com.outerspacemanager.outerspacemanager.activities.MainActivity"
            android:label="@string/app_name"/>
        <activity android:name=".activities.GalaxyActivity"
            android:parentActivityName="annaikanyouzoo.com.outerspacemanager.outerspacemanager.activities.MainActivity"
            android:label="@string/app_name"/>
    </application>

</manifest>